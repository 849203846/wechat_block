.header{
  width: 100%;
  position: relative;
  left: 0;
  top: 0;
}
.header>image{
  display: block;
  height: 3.6rem;
  width: 3.6rem;
  position: absolute;
  right: 1rem;
  top: 2rem;
  border-radius: 50px;
}
.header .headerTitle{
  margin-left: 1rem;
  padding-top: 1rem;
}
.header .headerTitle view{
  padding-top: 1rem;
  color: #aaaaaa;
  font-size: 1rem;
}
.header .headerTitle view:nth-child(1) text{
  color: purple;
  font-size: .8rem;

}
.header .headerTitle view:nth-child(1){
   font-weight: 500;
   font-size: 1.2rem;
   color: #333333;
}

.header .headerTitle image{
  width: 1rem;
  height: 1rem;
  margin-right: .5rem;
  vertical-align: top;
}
.header .headerTitle .weChatUserphone text{
  vertical-align: top;
}
.header .headerTitle .weChatUserphone image{
  margin-bottom: 1rem;
  margin-top: 5px;
}
.wantedstatus{
  height: 3.8rem;
  line-height: 3.8rem;
  margin-left: 1rem;
  margin-right: 1rem;
  border-bottom: 1px solid #eeeeee;
  border-top: 1px solid #eeeeee;
}
.title{
  width: 100%;
  height: 1rem;
  line-height: 1rem;
  margin-top: 1.4rem;
  margin-bottom: 1.4rem;
}
.title text:nth-child(1){
  display: inline-block;
  width: .5rem;
  height: 1rem;
  background: purple;
  /* margin-top: 1.5rem; */
  margin-left: 1rem;
  vertical-align: middle;
  margin-right: .6rem;
}
.title text:nth-child(2){
  font-weight: 500;
  padding-top: 1.5rem;
  vertical-align: middle;
}
.title text:nth-child(3){
  color: purple;
  vertical-align: middle;
  float: right;
  padding-right: 1rem;
}
.myadvantagetext{
  padding-left: 1rem;
  padding-right: 1rem;
  padding-bottom: 2rem;
  border-bottom: .36rem solid #fafafa;
}
.myintentionadd image{
  width: .92rem;
  height: .92rem;
  margin-right: .7rem;
  margin-top: 1rem;
  line-height: 3rem;
}
.myintentionadd{
  text-align: center;
  height: 3rem;  
  border-top: 1px solid #f2f2f2;
}
.myintentionbody{
  height: 5rem;

}

.myintentionbody view{
  margin-top: 1.8rem;
  margin-bottom: .5rem;
}

.myintentionbody view>text:nth-child(1){
  font-weight:500;
  text-align: left;
  font-size: 1rem;
  margin-left: 1rem;
}
.myintentionbody view>text:nth-child(2){
  text-align: right;
  color: purple;
  float: right;
  margin-right: 1rem;
}

.myintentionbody>text{
  font-size: .8rem;
  color: #a9a9a9;
  margin-left: 1rem;
  padding-top: .8rem;
}
.workExperiencebody{
  
  padding-bottom: 1rem;
}
.companyname text{
  font-size: 1rem;
  color: black;
  margin-left: 1rem;

}
.companyname navigator{
  float: right;
  color: purple;
  font-size: .8rem;
  margin-right: 1rem;
}
.companypost {
    margin-top: .3rem;  
    margin-bottom: .3rem;
}
.companypost text:nth-child(1){
   font-size: .8rem;
   margin-left: 1rem;
     color: #ababab;
}
.companypost text:nth-child(2){
  margin-right: 1rem;
  font-size: .8rem;
  color: #ababab;
  float: right;
}
.companycontent {
  margin-left: 1rem;
  font-size: .7rem;
  color: #ccc;
  margin-right: 1rem;
}

.tan{
  position: fixed;
  width: 100%;
  height: 100%;
  left: 0;
  right: 0;
  top:0;
  bottom:0;
  background: rgba(0,0,0,.4)
}

.tankuang{
  width: 17.5rem;
  background: white;
  position: fixed;
  left: 50%;
  top: 50%;
  margin-left: -8.75rem;
  margin-top: -6rem;
}
.tankuang view:nth-child(1){
  height: 3rem;
  line-height: 3rem;
  text-align: center;
}
.tankuang view:nth-child(1) text:nth-child(1){
  font-size: 1.2rem;
  margin-right: -2rem;
}
.tankuang view:nth-child(1) text:nth-child(2){
  color: #932fe9;
  float: right;
  margin-right: 1rem;
}
.tankuang input{
  height: 2.7rem;
  width: 80%;
  margin: 1rem auto;
  border: 1px solid #cccccc;
  text-align: center;
  
}
.tankuang view:nth-child(3){
  text-align: center;
  line-height: 2.8rem;
  height: 2.8rem;color: #932fe9;
  border-top: 1px solid #bababa;
  font-size: 1.2rem;
  display: flex;
}
.tankuang view:nth-child(3) view{
  flex: 1;
}