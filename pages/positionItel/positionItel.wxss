    text-align: center;
    font-size: 12px;

}
.xingqu{
    display: inline-block;
  width: 4.08rem;
  height: 2.6rem;
  text-align: center;
      font-size: 12px;
}
.goutong{
      display: inline-block;
 flex: 1;
  height: 2.6rem;
  text-align: center;
  background: purple;
  color: white;
    line-height: 2.6rem;
}
.header{
 width: 100%;
 height: 9.66rem;
 border-bottom:.36rem solid #fafafa; 
}
.headerone{
  height: 4.68rem;
  width: 90%;
  border-bottom: 1px solid #eeeeee;
  margin: .7rem auto 0;
}
.headerone view text:nth-child(1){

  font-size: 1.2rem;
}
.headerone view text:nth-child(2){
  margin-top: .2rem;
  color: purple;
  float: right;
  font-size: 1rem;
}
.headerone view:nth-child(2){
  margin-top: 1rem;
}
.headertwo{
    height: 4.68rem;
  width: 90%;
  margin: 1rem auto 0;
  position: relative;

}
.headertwo image{
  width: 2.5rem;
  height: 2.5rem;
  border-radius: 50px;
  margin-right: .8rem;
}
.headertwo view:nth-child(2){
 position: absolute;
   width: 80%;
 left: 3.3rem;
 top: .2rem;
 font-size: 1.2rem;
  
}
.headertwo view text:nth-child(2){
    position: absolute;
    right: 0;
    top: .2rem;
    font-size: 1rem;
    color: purple;
}
.headertwo view:nth-child(3){
  position: absolute;
   left: 3.3rem;
   top: 2rem;
  font-size: .8rem;
  color: #ababab
}