.footer{
  position: fixed;
  height: 2.6rem;
  width: 100%;
  border-top: 1px solid #cccccc;
  display: flex;
  bottom: 0;
  background: white;
}
.footer image{
  width: 1.2rem;
  height: 1.12rem;
  margin-top: 5px;
}
.fengxiang{
  width: 4.08rem;
  display: inline-block;
  height: 2.6rem;
    text-align: center;
    font-size: 12px;

}
.xingqu{
    display: inline-block;
  width: 4.08rem;
  height: 2.6rem;
  text-align: center;
      font-size: 12px;
}
.goutong{
      display: inline-block;
 flex: 1;
  height: 2.6rem;
  text-align: center;
  background: #f59800;
  color: white;
    line-height: 2.6rem;
}
.header{
 width: 100%;
 height: 9.66rem;
 border-bottom:.36rem solid #fafafa; 
}
.headerone{
  height: 4.68rem;
  width: 90%;
  border-bottom: 1px solid #eeeeee;
  margin: .7rem auto 0;
}
.headerone view text:nth-child(1){

  font-size: 1.2rem;
}
.headerone view text:nth-child(2){
  margin-top: .2rem;
  color: #f59800;
  float: right;
  font-size: 1rem;
}
.headerone view:nth-child(2){
  margin-top: 1rem;
}
.headertwo{
    height: 4.68rem;
  width: 90%;
  margin: 1rem auto 0;
  position: relative;

}
.headertwo image{
  width: 2.5rem;
  height: 2.5rem;
  border-radius: 50px;
  margin-right: .8rem;
}
.headertwo view:nth-child(2){
 position: absolute;
   width: 80%;
 left: 3.3rem;
 top: .2rem;
 font-size: 1.2rem;
  
}
.headertwo view text:nth-child(2){
    position: absolute;
    right: 0;
    top: .2rem;
    font-size: 1rem;
    color: #f59800;
}
.headertwo view:nth-child(3){
  position: absolute;
   left: 3.3rem;
   top: 2rem;
  font-size: .8rem;
  color: #ababab
}

.title{
  width: 100%;
  height: 1rem;
  line-height: 1rem;
  margin-top: 1.4rem;
  margin-bottom: 1.4rem;
}
.title text:nth-child(1){
  display: inline-block;
  width: .5rem;
  height: 1rem;
  background: #f59800;
  /* margin-top: 1.5rem; */
  margin-left: 1rem;
  vertical-align: middle;
  margin-right: .6rem;
}
.title text:nth-child(2){
  font-weight: 500;
  padding-top: 1.5rem;
  vertical-align: middle;
}
.title text:nth-child(3){
  color: #f59800;
  vertical-align: middle;
  float: right;
  padding-right: 1rem;
}
.description{
  width: 90%;
  margin: 0 auto;
}
.descriptiontitle{
  margin-bottom: 2.2rem;
}
.SkillLbael{
  display: inline-block;
  border: 1px solid #c6c6c6;
  text-align: center;
  padding:.2rem .5rem;
  margin-left: 1rem;
  border-radius: 5px;
}

.company{
  height: 4.5rem;
  width: 90%;
  border-bottom: 1px solid #eee;
  position: relative;
  margin: 0 auto;
}
.companylogo{
  width: 2.5rem;
  height: 2.5rem;
  border-radius: 5px;
  margin-top: .9rem;
}
.companyname{
  position: absolute;
  top: 0.9rem;
  left: 3.2rem;

}
.CompanyProfile{
    position: absolute;
  top: 2.5rem;
  left: 3.2rem;
  font-size: .8rem;
  color: #ababaB;
}
.WorkingRess{
  width: 90%;
  margin: 0 auto;
  padding-bottom: 1.4rem;
  border-bottom: 1rem solid #fafafa;
}
.WorkingRess text{
  font-size: .8rem;
  color: #ababaB

}
.WorkingRess image{
  width: .4rem;
  height: .7rem;
  float: right;
  margin-top: .4rem;
  vertical-align: middle;
}